    "prestart": "bb-ang copy-assets",
    "start": "ng serve",
    "test": "ng test",
    "update": "ng update @bb-cli/schematics@^2",
    "api:check": "bb-ang check-api",
    "api:extract": "bb-ang extract-api",
    "start:proxy": "ng serve --proxy-config proxy.conf.json"
  },
  "private": true,
