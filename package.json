  "license": "MIT",
  "scripts": {
    "build": "ng build",
    "e2e": "ng e2e",
    "format": "prettier --write \"{apps,libs}/**/*.ts\"",
    "format:check": "prettier --list-different \"{apps,libs}/**/*.ts\"",
    "lint": "ng lint",
    "ng": "ng",
    "package:apps": "bb-ang package-apps --output-hashing --build-memory=4096 --extend-container",
